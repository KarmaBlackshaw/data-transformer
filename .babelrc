{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            "edge >= 16",
            "safari >= 9",
            "firefox >= 57",
            "ie >= 10",
            "ios >= 9",
            "chrome >= 49"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
